{
  "name": "browser-code-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "resetMocks": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "/utils/utilityFunctions.ts",
      "/data/testLogic/testScripts/*"
    ]
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.0.2",
    "@uiw/react-codemirror": "^4.11.4",
    "axios": "^0.27.2",
    "next": "12.2.4",
    "re-resizable": "^6.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.48.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.3",
    "vm2": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/react": "18.0.15",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "jest-localstorage-mock": "^2.4.21",
    "node-mocks-http": "^1.11.0",
    "ts-jest": "^28.0.5",
    "typescript": "4.7.4"
  }
}
